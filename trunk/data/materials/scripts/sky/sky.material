
material NoonSkyBox
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			texture_unit
			{
				cubic_texture mysky.jpg separateUV
				tex_address_mode clamp
			}
		}
	}
}

material CloudySky
{
	technique
	{
		pass
		{
			lighting off
			depth_write off

			texture_unit
			{
				texture cartoon_cloudy_sky.png
				scroll_anim 0.05 0
			}
		}
	}
}
