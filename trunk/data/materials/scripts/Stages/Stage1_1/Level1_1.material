
material Ground
{
	technique Toon
	{
		pass
		{
			vertex_program_ref ToonVP
			{
				param_named shininess float4 35.0 1.0 1.0 1.0
			}
			
			fragment_program_ref ToonFP
			{
        param_named diffuse float4 0.244 0.244 0.244 1
        param_named specular float4 0.4 0.4 0.4 1
			}

      // Textures for shades
			texture_unit
			{
				texture toon.png 2d
				tex_address_mode clamp
				filtering none
			}

      // Real Texture
			texture_unit
			{
				texture Island.png
			}
		}
	}
	
	technique
	{
		pass
		{
			ambient 0 0 0 1

			texture_unit
			{
				texture Island.png
			}
		}

	}

}



